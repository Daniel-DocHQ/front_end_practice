{
	"parser": "babel-eslint",
	"env": {
		"es6": true,
		"browser": true,
		"node": true
	},
	"rules": {
		"semi": "off",
		"indent": "off",
		"no-tabs": "off",
		"quotes": "off",
		"no-trailing-spaces": "off",
		"comma-dangle": "off",
		"arrow-parens": "off",
		"require-jsdoc": "off"
		
	},
	"extends": ["eslint:recommended", "google"]
}